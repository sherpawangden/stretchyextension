{
    "name": "stretchy",
    "version": "0.0.4",
    "description": "An extension to set timed intervals to take a break and stretch",
    "manifest_version": 2,
    "background" : {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "16": "16icon.png",
            "48": "48icon.png",
            "128":"128icon.png"
            },
        "default_popup": "popup.html",
        "default_title": "Stretch From Home"
    },
    "permissions": ["notifications", "alarms", "storage","background"]
}
